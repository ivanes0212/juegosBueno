{% extends 'uniJuegosBundle::layout.html.twig' %}


{% block cuerpo -%}
    <center><h1>{{ entity.tituloNot }}</h1></center>

    <table class="record_properties">
        <tbody>
                       
            <tr>
                
                <td>{{ entity.textoNot }}</td>
            </tr>
            <tr>
                
                <td><center><img src="{{ asset('bundles/unijuegos/imagenes/'~ entity.fotoNot  ~ '') }}" width=500px height=300px/></center></td>
            </tr>
              
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('noticias') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('noticias_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
