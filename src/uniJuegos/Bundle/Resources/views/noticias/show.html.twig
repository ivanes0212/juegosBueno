{% extends 'uniJuegosBundle::layout.html.twig' %}


{% block cuerpo -%}
    <center><h1>{{ entity.tituloNot }}</h1></center>

    
       
                <div id="texto">
                {{ entity.textoNot }}
                </div>
                <br>
                
                <center><img src="{{ asset('bundles/unijuegos/imagenes/'~ entity.fotoNot  ~ '') }}" width=500px height=300px/></center>
           
              
      
<div id="edit">
        <ul class="record_actions">
    <li>
        <a href="{{ path('noticias') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('noticias_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
</div>
{% endblock %}
